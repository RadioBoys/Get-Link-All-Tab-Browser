{
  "manifest_version": 3,
  "name": "Tab Counter and Link Collector",
  "version": "1.0",
  "permissions": ["tabs", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "image16.png",
      "32": "image32.png",
      "48": "image48.png",
      "128": "image128.png"
    }
  }
}